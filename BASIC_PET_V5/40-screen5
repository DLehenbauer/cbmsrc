	.BYT $28,$05,$19,$21
	.BYT $00,$07,$00,$00
	.BYT $3C,$00,$00,$00
	.BYT $00,$00
.SKI 3
.SKI 2
; ****** TONE TABLE FOR BELL ******
.SKI 2
TONES	.BYT 14,30,62,126
	.BYT 62,30,14
.SKI 3
