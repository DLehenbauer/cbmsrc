	.BYT $14,$11,$FF,$29,$5C,$27,$24,$22
	.BYT $1D,$13,$5F,$28,$26,$25,$23,$21
RUNTB	.BYT 'D',$CC,'"*',$D,'RUN',$D
.SKI 2
; ****** 6845 CRTC TEXT MODE ******
.SKI 2
TEXT	.BYT $31,$28,$29,$0F
	.BYT $20,$03,$19,$1D
	.BYT $00,$09,$00,$00
	.BYT $20,$00,$00,$00
	.BYT $00,$00
.SKI 2
; ****** 6845 CRTC GRAPHIC MODE ******
.SKI 2
