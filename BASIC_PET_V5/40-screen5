.PAG 'GRAPHICHAR1'
CHARB	.BYT $3D,$2E
	.BYT $FF,$3,$3C,$20,$5B,$12
	.BYT $2D,$30,0,$3E,$FF,$5D,$40,0
	.BYT $2B,$32,$FF,$3F,$2C,$4E,$56,$58
