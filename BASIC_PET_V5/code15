.PAG 'CODE15'
CAT	LDA FACLO
	PHA
	LDA FACMO
	PHA
	JSR EVAL
	JSR CHKSTR
	PLA
	STA STRNG1
	PLA
	STA STRNG1+1
	LDY #0
	LDA (STRNG1)Y
	CLC
	ADC (FACMO)Y
	BCC SIZEOK
	LDX #ERRLS
	JMP ERROR
SIZEOK	JSR STRINI
	JSR MOVINS
	LDA DSCPNT
	LDY DSCPNT+1
	JSR FRETMP
	JSR MOVDO
	LDA STRNG1
	LDY STRNG1+1
	JSR FRETMP
	JSR PUTNEW
	JMP BTSTOP
MOVINS	LDY #0
	LDA (STRNG1)Y
	PHA
	INY
.END
